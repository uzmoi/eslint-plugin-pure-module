{
    "compilerOptions": {
        "module": "ES2015",
        "moduleResolution": "Node",
        "allowSyntheticDefaultImports": true,
        "target": "ES2020",
        "lib": ["ES2020"],
        "skipLibCheck": true,
        "strict": true,
        "noEmit": true,
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}