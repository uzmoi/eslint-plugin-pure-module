{
    "compilerOptions": {
        "module": "Node16",
        "moduleResolution": "Node16",
        "resolveJsonModule": true,
        "target": "ES2020",
        "lib": ["ES2020"],
        "skipLibCheck": true,
        "strict": true,
        "noEmit": true,
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}